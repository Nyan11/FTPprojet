package cda.projet.serveur;

import java.io.File;
import java.io.IOException;
import java.io.PrintStream;
import java.net.ServerSocket;
import java.net.Socket;

public class CommandeGET extends Commande {
	
	public static int port = 5000;
	
	public CommandeGET(PrintStream ps, String commandeStr) {
		super(ps, commandeStr);
	}

	public void execute() {
		File file = new File(CommandExecutor.userPos, this.commandeArgs[0]);
		if(!file.exists()) {
			ps.println("2 le fichier n'existe pas");
		}
		
		ServerSocket serveurFTP;
		Socket socket;
		try {
			serveurFTP = new ServerSocket(port++);
			socket = serveurFTP.accept();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		ps.println("La commande get n'est pas encoré implémentée");
	}

}
