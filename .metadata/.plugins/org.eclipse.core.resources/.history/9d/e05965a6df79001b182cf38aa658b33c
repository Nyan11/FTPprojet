package cda.ftp.ihm.components;

import javafx.scene.layout.VBox;

public class FTPappView extends VBox {
	
	public FTPappView() {
		super();
		generateView();
	}

	private void generateView() {
        FTPdirView dir = new FTPdirView();
        FTPmenuView menu = new FTPmenuView();
        FTPactionView action = new FTPactionView();
        FTPselectView select = new FTPselectView();
        
        this.getChildren().add(menu.generateView());
        this.getChildren().add(dir.generateView());
        this.getChildren().add(select);
        this.getChildren().add(action);
	}
}
