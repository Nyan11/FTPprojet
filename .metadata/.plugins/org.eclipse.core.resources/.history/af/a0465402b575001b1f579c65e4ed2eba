package cda.projet.serveur;

import java.io.File;
import java.io.PrintStream;

public class CommandeSTOR extends Commande {
	
	public CommandeSTOR(PrintStream ps, String commandeStr) {
		super(ps, commandeStr);
	}

	public void execute() {
		File dir = new File(CommandExecutor.userPos, commandeArgs[0].toLowerCase());
		if(dir.exists()) {
			if(dir.isDirectory()) {
				CommandExecutor.userPos = dir;
				ps.println("0 ok");
			}
			else {
				ps.println("2 file is not a directory");
			}
		}
		else {
			ps.println("2 directory don't exist");
		}
	}

}
