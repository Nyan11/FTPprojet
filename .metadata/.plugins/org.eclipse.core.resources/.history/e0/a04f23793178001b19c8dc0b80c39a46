package cda.ftp.ihm.components;

import javafx.scene.control.Button;
import javafx.scene.control.TextField;
import javafx.scene.layout.VBox;

public class FTPiconView {
	public VBox generateView() {
		VBox mainBox = new VBox();
		TextField currentPath = new TextField();
		Button previousButton = new Button("‚Üê");
		
		mainBox.getChildren().add(previousButton);
		mainBox.getChildren().add(currentPath);
		
		return mainBox;
	}
}
