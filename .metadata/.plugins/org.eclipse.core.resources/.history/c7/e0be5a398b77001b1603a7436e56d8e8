package cda.projet.serveur.specials;

import java.io.File;
import java.net.Socket;

import cda.projet.serveur.WorkerClient;

public class Uploader implements Runnable {
	
	protected WorkerClient client;
	protected Socket clientSocket;
	protected File file;
	protected static int port = 4000;
	
	public Uploader(WorkerClient client, Socket socket, File file) {
		this.client = client;
		this.clientSocket = socket;
	}
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}
	
	public synchronized int accessPort() {
		return Uploader.port++;
	}
}
